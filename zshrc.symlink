if [ -d "$HOME/.bin" ]; then
  path=($HOME/.bin $path)
fi

export EDITOR='vim -f'

eval "$(rbenv init - --no-rehash)"

plugins=(
  /usr/local/share/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh
  /usr/local/share/liquidprompt
  /usr/local/etc/profile.d/z.sh
  $HOME/.dotfiles/**/*.zsh
)

for plugin in $plugins; do
  if [ -f "$plugin"  ]; then
    . "$plugin"
  fi
done
