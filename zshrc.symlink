if [ -d "$HOME/.bin" ]; then
  path=($HOME/.bin $path)
fi

source /usr/local/share/antigen/antigen.zsh

antigen use oh-my-zsh

antigen bundle git
antigen bundle zsh-users/zsh-syntax-highlighting
antigen bundle nojhan/liquidprompt
antigen bundle rupa/z

antigen apply

export EDITOR='vim -f'
alias v='vim'

eval "$(rbenv init -)"

pyenv() {
  eval "$(command pyenv init -)"
  pyenv "$@"
}
